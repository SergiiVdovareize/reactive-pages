{
  "name": "reactive-pages",
  "version": "0.1.1",
  "homepage": "https://reactive.vdovareize.me",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "gh-pages": "^6.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "docker:build": "docker build -t reactive-pages .",
    "docker:run": "docker run -p 3000:3000 reactive-pages",
